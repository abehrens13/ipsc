{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "mongo",
			"image": "mongo",
			"hostname": "mongo",
			"essential": true,
			"memory": null,
			"memoryReservation": 128
		},
		{
			"name": "mongoexpress",
			"image": "mongo-express",
			"hostname": "mongo",
			"essential": true,
			"memory": null,
			"memoryReservation": 128,
			"portMappings": [
				{
					"hostPort": 4001,
					"containerPort": 8081
				}
			],
			"links": [
				"mongo"
			]
		},
		{
			"name": "backend",
			"image": "feb18/backend",
			"hostname": "backend",
			"essential": true,
			"memory": null,
			"memoryReservation": 128,
			"portMappings": [
				{
					"hostPort": 8080,
					"containerPort": 8080
				}
			],
			"links": [
				"mongo"
			]
		}
	]
}
